<main class="main">
  <div class="inner">
      <div class="greeting">
      <% if user_signed_in? %>
        <%= "こんにちは、" %>
        <%= link_to "#{current_user.name}さん", root_path, class: :greeting__link %>
      <% else %>
        <% root_path %>
      <% end %>
      </div>
    <div class="card__wrapper">
      <%= render partial: 'prototype', collection: @prototypes %>
    </div>
  </div>
</main>
